<div class="container">
  <div id="resume_new" class="row">
    <h3>向<%= @job.title %>发送简历</h3>
    <hr>
    <%= simple_form_for [@job, @resume] do |f| %>
    <%= f.input :content, label:"内容:" %>
    <%= f.input :attachment, label:"附件:" %>
    <%= f.submit "发送简历", class:"btn btn-sm btn-count" %>
  <% end %>
</div>
</div>
