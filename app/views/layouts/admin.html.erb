<!DOCTYPE html>
<html>
  <head>
    <title>JobLists</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div class="container-fluid">
      <%= render "common/navbar" %>
      <%= render "common/flashes" %>
      <div class="row">
        <div class="col-md-1">
          <ul class="nav nav-pills nav-stacked" style="max-width: 300px; padding-top: 20px;">
            <li class="active"><%= link_to "后台首页", admin_jobs_path, class:"text-center" %></li>
            <li><%= link_to "前台首页", jobs_path, class:"text-center" %></li>
          </ul>
        </div>
        <div class="col-md-11">
          <%= yield %>
        </div>
      </div>
    </div>
    <%= render "common/footer" %>
  </body>
</html>









